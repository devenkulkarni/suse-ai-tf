# Specify the prefix for the ec2 instance
instance_prefix = null

# Specify the instance_type for the ec2 instance
instance_type = "g4dn.xlarge"

# Specify your public ssh key 
use_existing_ssh_public_key = false

# Specifies the full path where the pre-generated SSH PRIVATE key is located (not generated by Terraform)
user_ssh_private_key = null

# Specifies the full path where the pre-generated SSH PUBLIC key is located (not generated by Terraform)
user_ssh_public_key = null

# [Required] Specify the registration code for registering your SLE micro 6.1 instance with SUSE (Can be obtained from SCC.)
registration_code = null

# [Required] Specifies the name of the registry that can be used to download/pull SUSE AI stack images:
registry_name = "dp.apps.rancher.io"

# [Required] Specify the secretname that contains sensitive information for accessing image registry: 
registry_secretname = "application-collection"

# [Required] Specify the username for the image registry
registry_username = null

# [Required] Specify the password/token for the image registry
registry_password = null

# Specify the name of the namespace where you want the SUSE AI applications to be deployed:
suse_ai_namespace = "suse-ai"

# Specify the name of the namespace where you want the CERT_MANAGER to be deployed:
cert_manager_namespace = "suse-ai"

# Specify the name of the namespace where you want the GPU_OPERATOR to be deployed:
gpu_operator_ns = "gpu-operator"
